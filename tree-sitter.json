{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "valvecfg",
      "camelcase": "ValveCFG",
      "title": "Valve CFG",
      "scope": "source.valvecfg",
      "path": ".",
      "file-types": [
        "cfg"
      ],
      "highlights": [
        "queries/valvecfg/highlights.scm"
      ],
      "injection-regex": "^(valvecfg|cfg)$",
      "class-name": "TreeSitterValvecfg"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Tree sitter configuration for *.cfg files in Valve games.",
    "authors": [
      {
        "name": "Ilya G.",
        "url": "https://github.com/keycat"
      }
    ],
    "links": {
      "repository": "https://github.com/keycat/tree-sitter-valvecfg"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
